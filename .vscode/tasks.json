{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Rebuild VideoServices",
			"type": "shell",
			"command": "msbuild",
			"args": [
				"VideoServices.vcxproj",
				"/t:Rebuild",
				"/p:Configuration=Release",
				"/p:Platform=Win32",
				"/v:minimal"
			],
			"problemMatcher": [
				"$msCompile"
			]
		},
		{
			"label": "Build Game2 Only",
			"type": "shell",
			"command": "msbuild",
			"args": [
				"NetTarget/Game2/Game2.vcxproj",
				"/t:Rebuild",
				"/p:Configuration=Release",
				"/p:Platform=Win32",
				"/v:minimal"
			],
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build"
		}
	]
}